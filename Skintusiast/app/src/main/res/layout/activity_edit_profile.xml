<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_style"
    tools:context=".edit_profile_Activity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/back15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:contentDescription="back"
                android:onClick="back"
                android:paddingTop="20dp"
                android:paddingRight="50dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/baseline_arrow_back_ios_24" />

            <TextView
                android:id="@+id/textView183"
                android:layout_width="340dp"
                android:layout_height="2dp"
                android:background="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView148"
                app:layout_constraintTop_toBottomOf="@+id/textView148" />

            <TextView
                android:id="@+id/textView196"
                android:layout_width="340dp"
                android:layout_height="2dp"
                android:background="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView156"
                app:layout_constraintTop_toBottomOf="@+id/textView156" />

            <TextView
                android:id="@+id/textView195"
                android:layout_width="340dp"
                android:layout_height="2dp"
                android:background="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView154"
                app:layout_constraintTop_toBottomOf="@+id/textView154" />

            <TextView
                android:id="@+id/textView194"
                android:layout_width="340dp"
                android:layout_height="2dp"
                android:background="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView152"
                app:layout_constraintTop_toBottomOf="@+id/textView152" />

            <TextView
                android:id="@+id/textView193"
                android:layout_width="340dp"
                android:layout_height="2dp"
                android:background="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView150"
                app:layout_constraintTop_toBottomOf="@+id/textView150" />

            <Button
                android:id="@+id/btnsaveeditprofile"
                android:layout_width="139dp"
                android:layout_height="50dp"
                android:layout_marginStart="136dp"
                android:layout_marginEnd="136dp"
                android:layout_marginBottom="40dp"
                android:backgroundTint="#EE5DA7"
                android:text="Save"
                android:textColor="#FFFFFF"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:strokeColor="#EF5DA8"
                app:strokeWidth="1dp"
                tools:cornerRadius="20dp" />

            <Button
                android:id="@+id/button84"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:backgroundTint="#EF5DA8"
                android:text="Male"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/button83"
                app:layout_constraintStart_toEndOf="@+id/button83"
                app:layout_constraintTop_toTopOf="@+id/button83"
                app:strokeColor="#EF5DA8"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/button83"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:backgroundTint="#EF5DA8"
                android:text="Female"
                android:textColor="#FFFFFF"
                app:layout_constraintStart_toStartOf="@+id/textView157"
                app:layout_constraintTop_toBottomOf="@+id/textView157"
                app:strokeColor="#EF5DA8"
                app:strokeWidth="1dp" />

            <EditText
                android:id="@+id/textView150"
                android:layout_width="340dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:background="@null"
                android:gravity="center|left"
                android:hint="Email"
                android:inputType="text"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView149" />

            <EditText
                android:id="@+id/textView152"
                android:layout_width="340dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:background="@null"
                android:gravity="center|left"
                android:hint="********"
                android:inputType="textPassword"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView151" />

            <EditText
                android:id="@+id/textView154"
                android:layout_width="340dp"
                android:layout_height="48dp"
                android:background="@null"
                android:gravity="center|left"
                android:hint="4/15/2005"
                android:inputType="date"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView153"
                app:layout_constraintTop_toBottomOf="@+id/textView153" />

            <EditText
                android:id="@+id/textView156"
                android:layout_width="340dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:background="@null"
                android:gravity="center|left"
                android:hint="18"
                android:inputType="number"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView155" />

            <TextView
                android:id="@+id/textView149"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:text="Email"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView183" />

            <TextView
                android:id="@+id/textView151"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:text="Password"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView193" />

            <TextView
                android:id="@+id/textView153"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Date of Birth"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView152"
                app:layout_constraintTop_toBottomOf="@+id/textView194" />

            <TextView
                android:id="@+id/textView155"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:text="Age"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView195" />

            <TextView
                android:id="@+id/textView157"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Date of Birth"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView156"
                app:layout_constraintTop_toBottomOf="@+id/textView196" />

            <EditText
                android:id="@+id/textView148"
                android:layout_width="340dp"
                android:layout_height="48dp"
                android:background="@null"
                android:gravity="center|left"
                android:hint="Name"
                android:inputType="text"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView147"
                app:layout_constraintTop_toBottomOf="@+id/textView147" />

            <TextView
                android:id="@+id/textView147"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Name"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView146"
                app:layout_constraintTop_toBottomOf="@+id/textView146" />

            <TextView
                android:id="@+id/textView146"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Register"
                android:textColor="#000000"
                android:textSize="32dp"
                app:layout_constraintStart_toStartOf="@+id/back15"
                app:layout_constraintTop_toBottomOf="@+id/back15" />

            <TextView
                android:id="@+id/textView197"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginTop="131dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button83" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>