<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_style"
    tools:context=".HOME_your_skintype_1_Activity">

    <ImageView
        android:id="@+id/back8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:contentDescription="back"
        android:onClick="back"
        android:paddingTop="20dp"
        android:paddingRight="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_ios_24" />

    <Button
        android:id="@+id/btnskintype1"
        android:layout_width="139dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#EF5DA8"
        android:text="Next"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="#EF5DA8"
        app:strokeWidth="1dp"
        tools:cornerRadius="20dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:backgroundTint="#FFFFFF"
        android:text="Fresh and healthy, with small pores and not too many fine lines"
        android:textColor="#000000"
        android:textSize="11sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView28"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/button24"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FFFFFF"
        android:text="Dry, dull, and flaky skin"
        android:textColor="#000000"
        android:textSize="11sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="@+id/button17"
        app:layout_constraintTop_toBottomOf="@+id/button17"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/button25"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FFFFFF"
        android:text="Oily skin, appears shiny, and is prone to acne"
        android:textColor="#000000"
        android:textSize="11sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="@+id/button24"
        app:layout_constraintTop_toBottomOf="@+id/button24"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FFFFFF"
        android:text="Sensitive skin, that easily iritated and turns red when exposed to certain substances or extreme weather"
        android:textColor="#000000"
        android:textSize="11sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="@+id/button25"
        app:layout_constraintTop_toBottomOf="@+id/button25"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="1dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="You can select more than one"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/textView29"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="How do you describe your skin"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/back8"
        app:layout_constraintTop_toBottomOf="@+id/back8" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="29dp"
        android:layout_height="7dp"
        android:background="@drawable/roundcorner"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/btnskintype1"
        app:layout_constraintTop_toTopOf="@+id/textView33" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="29dp"
        android:layout_height="7dp"
        android:background="@drawable/roundcornerpink"
        android:text="TextView"
        android:textColor="#EF5DA8"
        app:layout_constraintBottom_toTopOf="@+id/btnskintype1"
        app:layout_constraintStart_toStartOf="@+id/btnskintype1" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="29dp"
        android:layout_height="7dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/roundcorner"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="@+id/textView32" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="29dp"
        android:layout_height="7dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/roundcorner"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/textView33"
        app:layout_constraintStart_toEndOf="@+id/textView31"
        app:layout_constraintTop_toTopOf="@+id/textView31" />

</androidx.constraintlayout.widget.ConstraintLayout>